@font-face {
  font-family: 'iconfont';  /* project id 1277785 */
  src: url('//at.alicdn.com/t/font_1277785_d91i34ehcy.eot');
  src: url('//at.alicdn.com/t/font_1277785_d91i34ehcy.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1277785_d91i34ehcy.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1277785_d91i34ehcy.woff') format('woff'),
  url('//at.alicdn.com/t/font_1277785_d91i34ehcy.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1277785_d91i34ehcy.svg#iconfont') format('svg');
}
.page
  table
    display: table;
    width: 100%;
    th
      text-align: left;
  .theme-default-content
    &:not(.custom)
      margin: 0 50px 0 80px;
      max-width: unset;
    code
      background-color: #fff5f5;
      color: #ff502c;
  .page-edit, .page-nav
    margin: 0 50px 0 80px;
    max-width: unset;
.iconfont 
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;

.customer-page-class
  color: #fb3;
  background-color: #333;
.navbar
  box-shadow: 0 2px 5px rgba(0,0,0,0.1);
.container
  margin: 20px 0;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  & > div
    flex: 1;
.theme-bg
  padding: 10px 20px;
  background-color: #282c34;
  color: #fb3;
  border-radius: 5px;